<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="560.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unipi.dii.inginf.dmml.soundhabit.controller.AdminPageController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefWidth="800.0" VBox.vgrow="ALWAYS">
      <children>
            <Label layoutX="250.0" layoutY="34.0" text="Add a new Song">
               <font>
                  <Font name="System Bold Italic" size="40.0" />
               </font>
            </Label>
            <Label layoutX="40.0" layoutY="137.0" text="Title:">
               <font>
                  <Font size="22.0" />
               </font>
            </Label>
            <TextField fx:id="songTitle" layoutX="108.0" layoutY="133.0" prefHeight="36.0" prefWidth="231.0" promptText="Insert song title...">
               <font>
                  <Font size="15.0" />
               </font>
            </TextField>
            <Label layoutX="423.0" layoutY="135.0" text="Author:">
               <font>
                  <Font size="22.0" />
               </font>
            </Label>
            <TextField fx:id="songAuthor" layoutX="521.0" layoutY="134.0" prefHeight="36.0" prefWidth="231.0" promptText="Insert author...">
               <font>
                  <Font size="15.0" />
               </font>
            </TextField>
            <Label layoutX="40.0" layoutY="220.0" text="Link to the song:">
               <font>
                  <Font size="22.0" />
               </font>
            </Label>
            <TextField fx:id="songUrl" layoutX="224.0" layoutY="218.0" prefHeight="36.0" prefWidth="541.0" promptText="Insert song URL...">
               <font>
                  <Font size="15.0" />
               </font>
            </TextField>
            <Label layoutX="40.0" layoutY="308.0" text="Link to the image:">
               <font>
                  <Font size="22.0" />
               </font>
            </Label>
            <TextField fx:id="imageUrl" layoutX="225.0" layoutY="306.0" prefHeight="36.0" prefWidth="541.0" promptText="Insert image URL...">
               <font>
                  <Font size="15.0" />
               </font>
            </TextField>
            <Label layoutX="138.0" layoutY="393.0" text="Genre(s):">
               <font>
                  <Font size="22.0" />
               </font>
            </Label>
            <Button fx:id="classifyButton" layoutX="551.0" layoutY="391.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="87.0" text="CLASSIFY">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="clearFieldsButton" layoutX="274.0" layoutY="488.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="117.0" text="Clear Fields">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="insertSongButton" layoutX="445.0" layoutY="488.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="117.0" text="Insert Song">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <CheckBox fx:id="bluesBox" layoutX="250.0" layoutY="383.0" mnemonicParsing="false" text="Blues">
               <font>
                  <Font size="15.0" />
               </font>
            </CheckBox>
            <CheckBox fx:id="classicalBox" layoutX="250.0" layoutY="419.0" mnemonicParsing="false" text="Classical">
               <font>
                  <Font size="15.0" />
               </font>
            </CheckBox>
            <CheckBox fx:id="jazzBox" layoutX="359.0" layoutY="383.0" mnemonicParsing="false" text="Jazz">
               <font>
                  <Font size="15.0" />
               </font>
            </CheckBox>
            <CheckBox fx:id="metalBox" layoutX="359.0" layoutY="419.0" mnemonicParsing="false" text="Metal">
               <font>
                  <Font size="15.0" />
               </font>
            </CheckBox>
            <CheckBox fx:id="popBox" layoutX="450.0" layoutY="383.0" mnemonicParsing="false" text="Pop">
               <font>
                  <Font size="15.0" />
               </font>
            </CheckBox>
            <CheckBox fx:id="rockBox" layoutX="450.0" layoutY="419.0" mnemonicParsing="false" text="Rock">
               <font>
                  <Font size="15.0" />
               </font>
            </CheckBox>
      </children>
    </AnchorPane>
  </children>
</VBox>
